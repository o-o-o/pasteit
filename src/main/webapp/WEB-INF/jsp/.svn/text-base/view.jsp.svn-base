<%@ page contentType="text/html; charset=UTF-8" %><%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Paste it!</title>
    <link href="/r/style.css" rel="stylesheet" type="text/css" />
    <link href="/r/prettify.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="/r/prettify.js"></script>
    <script type="text/javascript" src="/r/jquery-1.4.2.min.js"></script>
  </head>
  <script type="text/javascript">
$(function(){
	prettyPrint();
});
  </script>
  <body>
    <div id="topPan">
      <div id="logo"><a href="/"><img src="/r/logo.gif" title="Paste It!" alt="Paste It!" width="67" height="73" border="0" /></a></div>
      <h1>Paste it!</h1>
      <h2>paste your code.</h2>
      <a id="location" href="<c:out value="${location}" escapeXml="true" />"><c:out value="${location}" escapeXml="true" /></a>
      <pre id="code" class="prettyprint"><c:out value="${text}" escapeXml="true" /></pre>
    </div>
  </body>
</html>